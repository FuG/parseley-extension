{
  "manifest_version": 2,

  "name": "Parseley",
  "description": "This extension grabs html from amazon.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  },

//  "background": {
//    "persistent": false,
//    "scripts": ["scripts/background.js"]
//  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["scripts/content.js"]
    }
  ],

  "permissions": [
    "activeTab"
  ]
}